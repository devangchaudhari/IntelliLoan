{
    "version": 2,
    "builds": [
        {
            "src": "*app.py", "use": "@vercel/python"
        }
    ],
    "functions": {
    "app.py": {
      "memory": 1024,  
      "maxDuration": 10,
      "runtime": "python3.9"
    }
  },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
     
    ]
 
  }